{
  "name": "easypost",
  "vendor": "vtex",
  "version": "0.0.2",
  "title": "Easypost Integration",
  "description": "",
  "mustUpdateAt": "2022-08-28",
  "scripts": {
    "postreleasy": "vtex publish"
  },
  "dependencies": {
    "vtex.styleguide": "9.x"
  },
  "builders": {
    "react": "3.x",
    "admin": "0.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x",
    "node": "6.x",
    "graphql": "1.x"
  },
  "billingOptions": {
    "setupRoute": "/admin/app/easypost/",
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://help-tickets.vtex.com/en/support?app=vtex.easypost"
    },
    "free": true,
    "type": "free",
    "availableCountries": ["*"]
  },
  "policies": [
    {
      "name": "update-app-settings"
    },
    {
      "name": "POWER_USER_DS"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.easypost.com",
        "path": "/*"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
